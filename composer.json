{
	"name": "fab/media",
	"type": "typo3-cms-extension",
	"description": "Media management system for TYPO3 CMS.",
	"homepage": "https://github.com/fabarea/media",
	"license": [
		"GPL-2.0-or-later"
	],
	"keywords": [
		"TYPO3 CMS",
		"Media management"
	],
	"support": {
		"issues": "https://github.com/fabarea/media/issues"
	},
	"autoload": {
		"psr-4": {
			"Fab\\Media\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "media"
		}
	},
	"suggest": {
		"laravel/pint": "^1.8"
	},
	"require": {
		"typo3/cms-core": "^11",
		"fab/vidi": "~5.2 || ^6"
	},
	"config": {
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	}
}
